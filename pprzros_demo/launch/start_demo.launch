<launch>
	<!-- vim: set ft=xml noet : -->
	<!-- demo launch file-->
	<arg name="ac_id" />

	<node pkg="pprzros" type="pprzros_run" name="pprzros" output="screen"/>
	<node pkg="pprzros_demo" type="guided_mode_demo" name="pprzros_demo" clear_params="true" output="screen">
